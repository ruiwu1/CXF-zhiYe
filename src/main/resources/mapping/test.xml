<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.CXF.mapper.TestMapper">

    <select id="selectPassWdByLoginID" parameterType="java.lang.String" resultType="java.lang.String">
        SELECT UserPWD FROM IU_UsersLogin
        WHERE UserLoginID = #{loginID}
    </select>

</mapper>